{
    "name": "pnpm-monorepo",
    "private": true,
    "version": "1.0.0",
    "workspaces": [
        "scoped-observer/*",
        "behavior/*",
        "med1802/*"
    ],
    "devDependencies": {
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.0.11",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "react": "^18.3.1"
    },
    "scripts": {
        "build:med1802-scoped-observer": "pnpm --filter @med1802/scoped-observer run build",
        "publish:med1802-scoped-observer": "pnpm publish --filter @med1802/scoped-observer --access public",
        "build:med1802-quantum-ui": "pnpm --filter @med1802/quantum-ui run build",
        "publish:med1802-quantum-ui": "pnpm publish --filter @med1802/quantum-ui --access public",
        "build:med1802-quantum-toggle": "pnpm --filter @med1802/quantum-toggle run build",
        "publish:med1802-quantum-toggle": "pnpm publish --filter @med1802/quantum-toggle --access public",
        "build:med1802-quantum-scrollium": "pnpm --filter @med1802/quantum-scrollium run build",
        "publish:med1802-quantum-scrollium": "pnpm publish --filter @med1802/quantum-scrollium --access public"
    }
}